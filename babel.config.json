{
  "presets": [
    "@babel/preset-typescript",
    [
      "@babel/preset-env",
      {
        "corejs": 3,
        "debug": false,
        "modules": false,
        "useBuiltIns": "usage",
        "targets": {
          "esmodules": true,
          "node": "current"
        }
      }
    ]
  ],
  "plugins": [
    [
      "babel-plugin-add-import-extension",
      {
        "extension": "js",
        "replace": true,
        "observedScriptExtensions": ["js", "cjs", "mjs", "ts", "jsx", "tsx"]
      }
    ]
  ]
}
